<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".emergency_phone">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="비상 연락처"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="250dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="209dp"
        android:layout_height="50dp"
        android:backgroundTint="#787070"
        android:text="확인"
        android:textColorHighlight="#807171"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="484dp" />

    <EditText
        android:id="@+id/editTextTextPassword12"
        android:layout_width="167dp"
        android:layout_height="38dp"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="163dp"
        tools:layout_editor_absoluteY="242dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="비상 연락처 등록"
        android:textAllCaps="false"
        android:textSize="36sp"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="132dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="53dp"
        android:layout_height="58dp"
        android:clickable="true"
        app:backgroundTint="#B1A9A9"
        app:srcCompat="@android:drawable/ic_menu_add"
        tools:layout_editor_absoluteX="179dp"
        tools:layout_editor_absoluteY="302dp" />

</androidx.constraintlayout.widget.ConstraintLayout>